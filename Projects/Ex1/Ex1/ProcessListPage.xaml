<Window x:Class="Ex1.ProcessListPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ex1"
        mc:Ignorable="d"
        Title="ProcessListPage" Height="300.546" Width="300" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="0,0,2,-56">
        <TextBox Name="txtInput" Margin="5,10" MinLines="10">
        </TextBox>
        <Button Margin="5,5"  Content="Get all running app" Click="Getlist_Click"/>
        <Button Margin="5,5"  Content="Back to Home Screen" Click="Back_Click"/>
    </StackPanel>
</Window>
